@model dynamic

@{
    ViewBag.Title = "Multigame";

}

<br />
<br />
<div id="multiBattleDiv">
    <div class="row" data-bind="foreach: matchUps">
        <div class="panel panel-primary">
            <div class="panel-heading" > <span data-bind="text: PlayerTag"></span>  - <span data-bind="text: TotalScore"></span></div>
            <div class="panel-body col-md-12">
                <div class="col-md-5">
                    <h4>Games where I am playing first</h4>
                </div>
                <div class=" col-md-6">
                    <h4>Games where I am playing last</h4>
                </div>
                <div class="col-md-5">
                    <table class="table table-condensed">
                        <thead>
                            <th>Opponent</th>
                            <th>Win</th>
                            <th>Lost</th>
                            <th>Draw</th>
                        </thead>
                        <tbody data-bind="foreach: GamesWhereImPlayingFirst">
                            <tr>
                                <td data-bind="text: Player2Tag"></td>
                                <td data-bind="text: Xwins"></td>
                                <td data-bind="text: Owins"></td>
                                <td data-bind="text: Draws"></td>
                            </tr>

                        </tbody>
                        
                            <tr>
                                <td></td>
                                <td data-bind="text: TotalWinFirst"></td>
                                <td data-bind="text: TotalLostFirst"></td>
                                <td data-bind="text: TotalDrawFirst"></td>
                            </tr>
                        
                    </table>
                </div>
                <div class="col-md-6">
                    <table class="table table-condensed">
                        <thead>
                        <th>Opponent</th>
                        <th>Win</th>
                        <th>Lost</th>
                        <th>Draw</th>
                        </thead>
                        <tbody data-bind="foreach: GamesWhereImPlayingSecond">
                            <tr>
                                <td data-bind="text: Player1Tag"></td>
                                <td data-bind="text: Owins"></td>
                                <td data-bind="text: Xwins"></td>
                                <td data-bind="text: Draws"></td>
                            </tr>
                            
                        </tbody>
                        <tr>
                            <td></td>
                            <td data-bind="text: TotalWinLast"></td>
                            <td data-bind="text: TotalLostLast"></td>
                            <td data-bind="text: TotalDrawLast"></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
